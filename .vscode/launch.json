{
    "version": "0.2.0",
    "configurations": [      
      {
        "name": "Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true
      },
      {
          "name": "Debug Transcribe Script",
          "type": "node",
          "request": "launch",
          "program": "${workspaceFolder}/src/util/fetch_scripts.js", // Path to transcribe.js
          "skipFiles": ["<node_internals>/**"],
          "console": "integratedTerminal"
      }
    ]
  }