{
    "version": "0.2.0",
    "compounds": [
      {
        "name": "Debug Full Stack",
        "configurations": ["Chrome", "Node.js"]
      }
    ],    
    "configurations": [            
      {
        "name": "Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true
      },
      {
        "name": "Node.js",
        "type": "node",
        "request": "attach",
        "port": 9229,
        "restart": true,
        "sourceMaps": true,
        "skipFiles": ["<node_internals>/**"],
        "autoAttachChildProcesses": true
      },      
      {
          "name": "Debug Transcribe Script",
          "type": "node",
          "request": "launch",
          "runtimeExecutable": "C:/Users/I058700/AppData/Local/fnm_multishells/36192_1739597603088/node.exe",           
          "program": "${workspaceFolder}/src/utils/fetch_scripts.js", // Path to transcribe.js
          "skipFiles": ["<node_internals>/**"],
          "console": "integratedTerminal"
      }
    ]
  }